{
  "manifest_version": 3,
  "name": "Web Dataset Analyzer",
  "description": "Extract datasets from web pages with stats and quality insights",
  "version": "1.0",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/17353191.png",
      "48": "icons/17353191.png",
      "128": "icons/17353191.png"
    }
  },
  "icons": {
    "16": "icons/17353191.png",
    "48": "icons/17353191.png",
    "128": "icons/17353191.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ]
}
